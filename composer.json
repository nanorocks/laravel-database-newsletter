{
    "name": "nanorocks/laravel-database-newsletter",
    "description": "Laravel Newsletter package with built-in database driver support",
    "keywords": [
        "laravel",
        "newsletter",
        "database",
        "nanorocks"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nanorocks",
            "email": "andrejnankov@gmail.com",
            "homepage": "https://nankov.mk"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Nanorocks\\DatabaseNewsletter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nanorocks\\DatabaseNewsletter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nanorocks\\DatabaseNewsletter\\NewsletterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.6"
    }
}